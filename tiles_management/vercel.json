{
    "builds": [{
        "src": "tiles_management/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "Python 3.12.5" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "tiles_management/wsgi.py"
        }
    ]
}